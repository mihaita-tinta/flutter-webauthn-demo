+  registerNewUser(String firstName, String lastName) async {
+    if (isLoading) {
+      log('registerNewUser - ignored tap');
+      return;
+    }
+
+    setState(() {
+      isLoading = true;
+    });
+
+    try {
+      log('registerNewUser - $firstName $lastName');
+
+      var displayName = '$firstName $lastName';
+      UserRepository.saveDisplayName(displayName);
+      _registerOptions = await _api.registerRequest(firstName, lastName);
+      var username = _registerOptions!.username;
+
+      Fido2Client f = Fido2Client();
+      Map<String, dynamic> options = {
+        'username': _registerOptions?.username,
+        'rpDomain': _registerOptions?.rpId,
+        'rpName': _registerOptions?.rpName,
+        'coseAlgoValue': '${_registerOptions?.algoId}',
+      };
+      var challenge2 = _registerOptions?.challenge;
+      var userId = _registerOptions?.userId;
+      Future<RegistrationResult> res =
+          f.initiateRegistration(challenge2!, userId!, options);
+      res.catchError((e) {
+        showError(context: context, message: 'registerNewUser - Error $e');
+        isLoading = false;
+      });
+      res.timeout(const Duration(seconds: 20));
+      log('registerNewUser - waiting for result');
+      RegistrationResult r = await res;
+
+      await KeyRepository.storeKeyHandle(r.keyHandle);
+      await KeyRepository.storeUserHandle(userId);
+      await _api.registerResponse(
+          _registerOptions!.registrationId,
+          challenge2,
+          r.keyHandle,
+          r.clientData,
+          r.attestationObj);
+
+      log('registerNewUser - generated username: $username, userId: $userId');
+      showMessage(
+          context: context,
+          message: 'Successfully created your account $displayName!');
+    } finally {
+      setState(() {
+        isLoading = false;
+      });
+    }
+  }
+}
