+                    } else {
+                      try {
+                        await registerNewUser(firstNameController.value.text,
+                            lastNameController.value.text);
+
+                        if (mounted) {
+                          Navigator.push(
+                            context,
+                            MaterialPageRoute(
+                              builder: (context) => const LoginScreen(),
+                            ),
+                          );
+                        }
+                      } catch (error) {
+                        log('got exception $error');
+                        showError(
