+import 'package:flutter/material.dart';
+import 'package:rsocket/payload.dart';
+import 'package:rsocket/rsocket.dart';
+
+class RSocketText extends StatelessWidget {
+
+  final RSocket rSocket;
+
+  RSocketText(this.rSocket);
+
+  @override
+  Widget build(BuildContext context) {
+    return FutureBuilder<Payload>(
+      future: rSocket.requestResponse!(Payload.fromText('text/plain', 'Ping')),
+      // a Future<String> or null
+      builder: (BuildContext context,
+          AsyncSnapshot<Payload> rsocket) {
+        switch (rsocket.connectionState) {
+          case ConnectionState.waiting:
+          case ConnectionState.none:
+            return const Text(
+              'Connecting',
+            );
+          default:
+            if (rsocket.hasError) {
+              return const Text(
+                'Error to connect',
+              );
+            } else {
+              return Text(
+                'Hi ${rsocket.data}!',
+              );
+            }
+        }
+      },
+    );
+  }
+
+}
