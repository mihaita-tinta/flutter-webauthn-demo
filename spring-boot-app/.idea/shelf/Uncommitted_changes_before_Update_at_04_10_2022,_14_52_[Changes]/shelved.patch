+  @override
+  Widget build(BuildContext context) {
+    return Scaffold(
+      appBar: AppBar(
+        title: Center(
+          child: Text(widget.title),
+        ),
+      ),
+      body: Center(
+        child: Column(
+          mainAxisAlignment: MainAxisAlignment.center,
+          children: <Widget>[
+            ElevatedButton.icon(
+              onPressed: () async {
+                User u = await login();
+                if (mounted) {
+                  Navigator.push(
+                    context,
+                    MaterialPageRoute(
+                      builder: (context) => WelcomeScreen(user: u),
+                    ),
+                  );
+                }
+              },
+              label: Text(isLoading ? 'Loading' : 'Login as  $displayName'),
+              icon: const Icon(
+                Icons.fingerprint,
+                color: Colors.white,
+                // size: 20,
+              ),
+            ),
+          ],
+        ),
+      ),
+    );
+  }
+
+  Future<User> login() async {
+    log('login');
+    String keyHandle = await KeyRepository.loadKeyHandle();
+    String userHandle = await KeyRepository.loadUserHandle();
+    if (keyHandle == null) {
+      showMessage(context: context, message: 'Please create a new account.');
+      return Future.error('Please create a new account.');
+    }
+    SigningOptions _signingOptions =
+        await _api.assertionStart(userHandle, keyHandle);
+    Fido2Client f = Fido2Client();
+    String kh = await KeyRepository.loadKeyHandle();
+    SigningResult res = await f.initiateSigning(
+        kh, _signingOptions.challenge, _signingOptions.rpId);
+    await KeyRepository.storeKeyHandle(res.keyHandle);
+    User u = await _api.assertionFinish(
+        _signingOptions.assertionId,
+        res.keyHandle,
+        _signingOptions.challenge,
+        res.clientData,
+        res.authData,
+        res.signature,
+        userHandle);
+
+    await UserRepository.save(u);
+    return u;
+  }
+}
